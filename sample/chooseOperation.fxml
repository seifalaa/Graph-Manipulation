<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="628.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChooseOperationController">
   <children>
      <RadioButton fx:id="gR" layoutX="33.0" layoutY="48.0" mnemonicParsing="false" text="Graph Representation">
         <toggleGroup>
            <ToggleGroup fx:id="g1" />
         </toggleGroup>
         <font>
            <Font size="17.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="gC" layoutX="33.0" layoutY="89.0" mnemonicParsing="false" text="Graph Coloring" toggleGroup="$g1">
         <font>
            <Font size="17.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="flury" layoutX="33.0" layoutY="132.0" mnemonicParsing="false" text="Fluery's Algorithm" toggleGroup="$g1">
         <font>
            <Font size="17.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="MST" layoutX="33.0" layoutY="174.0" mnemonicParsing="false" text="Minimum Spanning Tree" toggleGroup="$g1">
         <font>
            <Font size="17.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="MHC" layoutX="33.0" layoutY="218.0" mnemonicParsing="false" text="Minimum Hamilton Circuit" toggleGroup="$g1">
         <font>
            <Font size="17.0" />
         </font>
      </RadioButton>
      <Label layoutX="33.0" layoutY="14.0" text="Choose Operation:">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <Button layoutX="437.0" layoutY="361.0" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="56.0" text="Back" />
      <Button fx:id="chooseButton" layoutX="510.0" layoutY="361.0" mnemonicParsing="false" onAction="#choose" prefHeight="26.0" prefWidth="87.0" text="Choose" />
      <RadioButton fx:id="MHPC" layoutX="33.0" layoutY="260.0" mnemonicParsing="false" text="Hamilton Path &amp; Hamilton Circuit" toggleGroup="$g1">

         <font>
            <Font size="17.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="MaximumFlow" layoutX="33.0" layoutY="299.0" mnemonicParsing="false" text="Maximum Flow" toggleGroup="$g1">
         <font>
            <Font size="17.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="Dijkstra" layoutX="33.0" layoutY="334.0" mnemonicParsing="false" text="Dijkstra" toggleGroup="$g1">
         <font>
            <Font size="17.0" />
         </font>
      </RadioButton>
   </children>
</AnchorPane>
